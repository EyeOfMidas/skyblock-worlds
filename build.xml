<?xml version="1.0" encoding="UTF-8"?>
<project name="SkyboxWorld" default="makejar" basedir=".">
	<target name="makejar" description="Create a jar for the project">
		<property name="version" value="0.2"/>
		<mkdir dir="target">
		</mkdir>
		<jar destfile="target/${ant.project.name}_${version}.jar">
			<fileset dir="bin">
			</fileset>
			<fileset file="plugin.yml">
			</fileset>
		</jar>
	</target>
</project>
